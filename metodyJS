//1. Scitani cisel
function scitaniCisel (let a, b){
let vysledek = a + b;
return vysledek;
}
scitaniCisel(3,6);

//2. Odcitani cisel
function odcitaniCisel (let a, b){
let vysledek = a - b;
return vysledek;
}
odcitaniCisel(8,5);

//3. Deleni cisel
function deleniCisel (let a, b){
let vysledek = a % b;
return vysledek;
}
deleniCisel(4,2);

//4. Nasobeni cisel
function nasobeniCisel (let a, b){
let vysledek = a * b;
return vysledek;
}
nasobeniCisel(3,5);

//5. Odmocnina
const number = 25;
const squareRoot = Math.sqrt(number);

//6. Získání času
const currentDate = new Date();

//7. Získání náhodného čísla
const randomNumber = Math.random();

//8. Určení délky pole
const ovoce = ['jablko', 'banan', 'pomeranc'];
const arrayLength = ovoce.length;

//9. Jestli je člověk zletilý či nezletilý
const vek = 20;
const jeDospely = vek >= 18 ? 'Dospělý' : 'Nezletilý';
